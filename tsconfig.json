// https://www.typescriptlang.org/tsconfig
{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": ["es2019"],
    "module": "commonjs",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6",
  }
}
