{
  "private": true,
  "name": "storybook-for-react-integration-tests",
  "version": "3.1.0",
  "scripts": {
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "storybook": "start-storybook -p 6007",
    "storybook-ci": "start-storybook -p 6007 --ci --quiet",
    "storybook:axe": "yarn run build-storybook && axe-storybook",
    "storybook:axeOnly": "axe-storybook",
    "storybook:axeServer": "axe-storybook --storybook-address http://localhost:6007"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chanzuckerberg/axe-storybook-testing": "file:./..",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.13"
  }
}
