{
  "private": true,
  "name": "storybook-for-react-integration-tests",
  "version": "3.1.0",
  "scripts": {
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "storybook-ci": "start-storybook -p 6006 --ci --quiet",
    "storybook:axe": "build-storybook && axe-storybook",
    "storybook:axe-no-build": "axe-storybook",
    "storybook:axe-no-build:server": "axe-storybook --storybook-address http://localhost:6006 --headless"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "babel-loader": "^8.2.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.0"
  }
}
